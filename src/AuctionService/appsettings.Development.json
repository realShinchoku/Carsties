{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=${DB_HOST}:${DB_PORT};User Id=${DB_USER};Password=${DB_PASSWORD};Database=${DB_NAME};"
  },
  "IdentityServiceUrl": "${IDENTITY_SERVICE_URL}",
  "RabbitMq": {
    "Host": "${RABBITMQ_HOST}",
    "Username": "${RABBITMQ_USERNAME}",
    "Password": "${RABBITMQ_PASSWORD}"
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:7001"
      },
      "Grpc": {
        "Protocols": "Http2",
        "Url": "http://localhost:7777"
      }
    }
  }
}
